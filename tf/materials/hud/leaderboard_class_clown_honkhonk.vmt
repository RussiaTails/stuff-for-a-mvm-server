"UnlitGeneric"
{
	"$baseTexture" "hud/leaderboard_class_clown_honkhonk"
	"$vertexcolor" 1
	"$no_fullbright" 1
	"$ignorez" 1
	"%keywords" "tf"
	"$translucent" 1
   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "25"
      }
   }
}

